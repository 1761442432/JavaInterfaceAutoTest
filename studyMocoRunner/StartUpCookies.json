    "description": "这是一个获取cookies的接口，返回的信息中包含cookies！",
    "request": {
      "uri": "/getCookies",
      "method": "get"
    },
    "response": {
      "text": "获取cookies成功！",
      "cookies": {
        "login": "true"
      }
    }
  },
  {
    "description": "需要带cookies的get请求学习",
    "request": {
      "uri":"/getDome/Cookies",
      "method": "get",
      "cookies": {
       "login": "true"
      }
    },
    "response": {
      "status": 200,
      "text": "这是一个需要带cookies的get请求"
    }
  },
  {
    "description": "需要带cookie的post请求学习",
    "request": {
      "uri": "/postDome/Cookies",
      "method": "post",
      "cookies": {
        "login": "true"
      },
      "json": {
        "name": "zhangsan",
        "age": "18"
      }
    },
    "response": {
      "status": 200,
      "json": {
        "zhangsan": "success",
        "status": "1"
      }
    }
  }
