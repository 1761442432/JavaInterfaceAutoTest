[
  {
    "description": "这是一个带headers的post请求！",
    "request": {
      "uri": "/postDome/headers",
      "method": "post",
      "headers": {
        "Content-type": "application/json;charset=utf-8"
      },
      "json": {
        "name": "zhangsan",
        "age": "18"
      }
    },
    "response": {
      "json": {
        "姓名": "张三",
        "年龄": "18"
      }
    }

  }
]